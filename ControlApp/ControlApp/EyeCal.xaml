<Window x:Class="ControlApp.EyeCal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlApp"
        mc:Ignorable="d"
        Title="Eye-Servo Calibration" Height="450" Width="800">
    <Grid>
        <Button x:Name="up_button" Content="UP" HorizontalAlignment="Left" Margin="254,141,0,0" VerticalAlignment="Top" Width="56" Height="35" Click="Up_Click"/>
        <Button x:Name="left_button" Content="Left" HorizontalAlignment="Left" Margin="198,176,0,0" VerticalAlignment="Top" Width="56" Height="35" Click="Left_Click"/>
        <Button x:Name="right_button" Content="Down" HorizontalAlignment="Left" Margin="254,211,0,0" VerticalAlignment="Top" Width="56" Height="35" Click="Down_Click"/>
        <Button x:Name="down_button" Content="Right" HorizontalAlignment="Left" Margin="310,176,0,0" VerticalAlignment="Top" Width="56" Height="35" Click="Right_Click"/>
        <Button x:Name="B_button" Content="B" HorizontalAlignment="Left" Margin="508,196,0,0" VerticalAlignment="Top" Width="52" Click="B_Click"/>

    </Grid>
</Window>
